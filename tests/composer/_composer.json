    "scripts": {
        "post-install-cmd": [
            "bin/api ckeditor:install --clear=drop",
            "bin/api assets:install public"
        ],
        "test": [
            "@test:reset",
            "@test:run"
        ],
        "test:reset": [
            "@test:database:setup",
            "bin/api doctrine:fixtures:load --no-interaction --env=test",
            "bin/api app:stripe:sync-products --env=test",
            "bin/api enqueue:setup-broker --env=test",
            "bin/api cache:clear --env=test",
            "bin/api app:twig:cache --env=test",
            "bin/grpc app:twig:cache --env=test"
        ],
        "test:run": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/phpunit -c phpunit.xml"
        ],
        "test:run:coverage": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/phpunit -c phpunit.xml --coverage-html ./build/logs/report"
        ],
        "test:database:setup": [
            "Composer\\Config::disableProcessTimeout",
            "bin/api doctrine:database:drop --if-exists --no-interaction --force --env=test",
            "bin/api doctrine:database:create --no-interaction --env=test",
            "mysql -h $DB_HOST -P $DB_PORT -u root pnp < ./sql-snippet/setup-dump.sql",
            "bin/api messenger:setup-transports --no-interaction --env=test",
            "bin/api doctrine:migrations:migrate --no-interaction --allow-no-migration --quiet --env=test"
        ],
        "test:database:setup-dump": [
            "@test:database:setup",
            "mysqldump -h $DB_HOST -P $DB_PORT -u root pnp > ./sql-snippet/setup-dump.sql"
        ],
        "linter": [
            "@linter:php-cs-fixer",
            "@linter:phpstan"
        ],
        "linter:php-cs-fixer": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/php-cs-fixer fix -v"
        ],
        "linter:phpstan": [
            "vendor/bin/phpstan analyse"
        ],
        "phpstan:generate-baseline": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/phpstan analyse --generate-baseline"
        ],
        "migrations:migrate": [
            "bin/api doctrine:migrations:migrate --env=test"
        ],
        "migrations:diff": [
            "bin/api doctrine:migrations:diff --formatted --env=test"
        ],
        "beta:deploy:api": [
            "Composer\\Config::disableProcessTimeout",
            "deploy --environment beta --profile ugm --instance api"
        ],
        "beta:deploy:grpc": [
            "Composer\\Config::disableProcessTimeout",
            "deploy --environment beta --profile ugm --instance grpc"
        ],
        "beta:deploy:worker": [
            "Composer\\Config::disableProcessTimeout",
            "deploy --environment beta --profile ugm --instance worker"
        ],
        "rc:deploy:api": [
            "Composer\\Config::disableProcessTimeout",
            "deploy --environment rc --profile ugm --instance api"
        ],
        "rc:deploy:grpc": [
            "Composer\\Config::disableProcessTimeout",
            "deploy --environment rc --profile ugm --instance grpc"
        ],
        "rc:deploy:worker": [
            "Composer\\Config::disableProcessTimeout",
            "deploy --environment rc --profile ugm --instance worker"
        ],
        "prod:deploy:api": [
            "Composer\\Config::disableProcessTimeout",
            "deploy --environment prod --profile ugm --instance api"
        ],
        "prod:deploy:grpc": [
            "Composer\\Config::disableProcessTimeout",
            "deploy --environment prod --profile ugm --instance grpc"
        ],
        "prod:deploy:worker": [
            "Composer\\Config::disableProcessTimeout",
            "deploy --environment prod --profile ugm --instance worker"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "sort-packages": true,
        "preferred-install": {
            "*": "source"
        },
        "platform": {
            "ext-swoole": "4.4.17"
        },
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "symfony/flex": true,
            "endroid/installer": true,
            "bamarni/composer-bin-plugin": true
        }
    },
    "extra": {
        "bamarni-bin": {
            "forward-command": true,
            "target-directory": "sbin"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "^5.4"
        },
        "symfony-web-dir": "public",
        "symfony-var-dir": "var",
        "symfony-assets-install": "hard",
        "branch-alias": {
            "dev-master": "8.3-dev"
        },
        "enable-patching": true,
