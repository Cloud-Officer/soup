                "collection inline table header attributes": "https://github.com/supersmile2009/SonataAdminBundle/commit/d0dcb21e7aa82080e3dbb2b1e447aad295b38193.patch",
                "add model link type and collection improvement": "https://gist.githubusercontent.com/ArtemBrovko/66d511b35b34e3a4936855a32f4ac8dd/raw/e9798f5f1aa4ebda525fb81ea37256a65923d7f3/model-link.patch",
                "remove final from getClass": "https://github.com/supersmile2009/SonataAdminBundle/commit/c26d06e5f5d84735d5d93b799a5288c25352ed2f.patch"
            },
            "sonata-project/formatter-bundle": {
                "fix for 5.x branch": "https://gist.githubusercontent.com/ArtemBrovko/0eeeb9ba255ea9b409a48a3fada2d4f5/raw/daf6b6bad44db83008351fe5a02d11553e8683f4/SonataFormatterBundle.patch"
            },
            "sonata-project/user-bundle": {
                "remove final from UserManager": "https://github.com/Miloud-H/SonataUserBundle/commit/5807d1c635a90f979e1a044db424eb847f804ef1.patch"
            },
            "snc/redis-bundle": {
                "remove strict type until phpredis extension >= 5.3.3": "https://github.com/Webonaute/SncRedisBundle/commit/fdc2947f77f9f283f452acc11625af7e554aa059.patch"
            },
            "spiral/roadrunner-grpc": {
                "add MethodInterface" : "https://github.com/supersmile2009/roadrunner-grpc/commit/c9d1270eedee4a5b237f3ff5677a9cb2778e49b7.patch"
            },
            "symfony/twig-bridge": {
                "fix missing button help block": "https://gist.githubusercontent.com/ArtemBrovko/f5452bada70b00d103986cff571bd0ed/raw/79feb446413c67935ee7da796a55b37419c9d8e5/POR-774.patch"
            }
        }
    }
