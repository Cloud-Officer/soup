            "jms/serializer": {
                "add mixed types": "https://github.com/supersmile2009/serializer/commit/b504f4c81117ecce73e9965e01da8586573d0cfe.patch"
            },
            "knplabs/doctrine-behaviors": {
                "fix long unique index name": "patches/knplabs-doctrine-behaviors-src-event-subscriber-translatable-event-subscriber.patch"
            },
            "sonata-project/admin-bundle": {
