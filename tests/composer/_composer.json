            "enqueue/job-queue": {
                "add delay before calculate job status": "https://github.com/Webonaute/job-queue/commit/bab00903c98e649b4e1be83f7dc3552baebb26a8.patch",
                "fix json_array type for json": "patches/enqueue-job-queue-doctrine-mapping-job-orm-xml.patch"
